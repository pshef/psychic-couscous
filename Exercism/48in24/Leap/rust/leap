pub fn is_leap_year(year: u64) -> bool {
    if year % 4 == 0 {
        if year % 100 == 0 {
            if year % 400 == 0 {
                println!("leap year");
                return true;
            } else {
                println!("not a leap year");
                return false;
            }
        } else {
           println!("leap year");
           return true;
        }
    } else {
        println!("not a leap year");
        return false;
    }
}
